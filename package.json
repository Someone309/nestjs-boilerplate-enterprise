{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "NestJS Boilerplate for Enterprise with Clean Architecture",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infrastructure/persistence/typeorm/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/persistence/typeorm/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/persistence/typeorm/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/infrastructure/persistence/typeorm/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "db:seed": "ts-node -r tsconfig-paths/register src/infrastructure/persistence/typeorm/seeds/seed.ts",
    "generate": "hygen",
    "generate:module": "hygen module new",
    "generate:entity": "hygen entity new",
    "generate:usecase": "hygen usecase new",
    "setup": "node scripts/setup.mjs",
    "cleanup:db": "node scripts/cleanup.mjs --type=database",
    "cleanup:auth": "node scripts/cleanup.mjs --type=auth",
    "cleanup:feature": "node scripts/cleanup.mjs --type=feature"
  },
  "dependencies": {
    "@apollo/server": "^5.3.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@nestjs/apollo": "^13.2.3",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.12",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-prometheus": "^0.211.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@prisma/client": "6.19.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^6.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.1.5",
    "multer": "^2.0.2",
    "nestjs-i18n": "^10.6.0",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.17.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^4.10.4",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/supertest": "^6.0.2",
    "chalk": "^5.3.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "inquirer": "^9.2.0",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "ora": "^8.0.0",
    "prettier": "^3.4.2",
    "prisma": "6.19.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@infrastructure/(.*)$": "<rootDir>/infrastructure/$1",
      "^@presentation/(.*)$": "<rootDir>/presentation/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1"
    }
  }
}
